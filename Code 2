q10 = input("\n10) A common pet that likes to chase mice. \nyour answer: ").lower()
if q10 == "cat" and q9 == "elephant" and q8 == "goldfish" and q7 == "turtle" and q6 == "spider" and q5 == "monkey" and q4 == "lion" and q3 == "penguin" and q2 == "tiger" and q1 == "giraffe":
    time.sleep(1)
    print("\nHere is the complete Crossword Puzzle 🫰🧩")
    print("══════════════════════════════════{$̲̅(̲̅●̲̅)̲̅$̲̅}════════════════════════════════════")
    print("═                                    -----                                             ═")
    print("═                                  8 | G |                                             ═")
    print("═                            -----------------                                         ═")
    print("═                          4 | L | I | O | N |                                         ═")
    print("═                            -----------------                                         ═")
    print("═                                    | L |                                             ═")
    print("═                        -----       -----                                             ═")
    print("═                      2 | T |       | D |                                             ═")
    print("═                    -----------------------------                                     ═")
    print("═                  1 | G | I | R | A | F | F | E |                                     ═")
    print("═                    -----------------------------                                     ═")
    print("═                        | G |       | I |             9                               ═")
    print("═    -------------------------       -------------------------                         ═")
    print("═  7 | T | U | R | T | L | E |     6 | S | P | I | D | E | R |                         ═")
    print("═    -------------------------       -------------------------               -----     ═")
    print("═                        | R |       | H |           | L |                 5 | M |     ═")
    print("═                        -----       -----           -----                   -----     ═")
    print("═                                                    | E |                   | O |     ═")
    print("═                                                    -----------------------------     ═")
